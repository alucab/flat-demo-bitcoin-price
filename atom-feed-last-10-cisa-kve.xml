---
layout: none
title: Atom Feed Last 10 CISA KVE
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
<title type="text">{{ site.title }}</title>
<subtitle type="text">{{page.title}}</subtitle>
<generator uri="https://github.com/mojombo/jekyll">Jekyll</generator>
<link rel="self" type="application/atom+xml" href="{{ site.url }}/{{page.title|slugify}}.xml" />
<link rel="alternate" type="text/html" href="{{ site.url }}" />
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{ site.url }}/</id>
<author>
  <name>{{ site.author }}</name>
  <uri>{{ site.url }}/</uri>
  <email>{{ site.email }}</email>
</author>
{% assign sortedvuln = site.data.vuln | reverse  %}
{% for v in sortedvuln  limit:10%}

<entry>
  <title type="html"><![CDATA[{{ v.Name | cdata_escape }}]]></title>
  <link rel="alternate" type="text/html" href="https://nvd.nist.gov/vuln/detail/{{ v.CVE }}" />
  <id>https://nvd.nist.gov/vuln/detail/{{ v.CVE }}</id>
  <published>{{ v.Added }}T00:00:00Z</published>
  <updated>{{ v.Due }}T00:00:01Z</updated>
  <author>
    <name>{{ v.Vendor }}</name>
  </author>
  <summary type="html">{{ v.Description | xml_escape }}</summary>
  <content>{{ v | jsonify }}</content>
</entry>
{% endfor %}
</feed>
