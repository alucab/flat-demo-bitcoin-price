---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
<title type="text">{{ site.title }}</title>
<subtitle type="text">{{ site.tagline }}</subtitle>
<generator uri="https://github.com/mojombo/jekyll">Jekyll</generator>
<link rel="self" type="application/atom+xml" href="{{ site.url }}/feed.xml" />
<link rel="alternate" type="text/html" href="{{ site.url }}" />
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{ site.url }}/</id>
<author>
  <name>{{ site.owner.name }}</name>
  <uri>{{ site.url }}/</uri>
  <email>{{ site.owner.email }}</email>
</author>
{% for v in site.data.vuln limit:20 %}

<entry>
  <title type="html"><![CDATA[{{ v.Name | cdata_escape }}]]></title>
  <link rel="alternate" type="text/html" href="https://nvd.nist.gov/vuln/detail/{{ v.CVE }}" />
  <id>https://nvd.nist.gov/vuln/detail/{{ v.CVE }}</id>
  <published>{{ v.Added }}</published>
  <updated>{{ v.Due }}</updated>
  <author>
    <name>{{ v.Vendor }}</name>
  </author>
  <content type="html">{{ v.Description | xml_escape }}</content>
</entry>
{% endfor %}
</feed>
